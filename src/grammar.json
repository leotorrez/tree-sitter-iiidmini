                        ]
                      },
                      {
                        "type": "SEQ",
                        "members": [
                          {
                            "type": "PATTERN",
                            "value": "[eE]"
                          },
                          {
                            "type": "PATTERN",
                            "value": "[+-]?[0-9](_?[0-9])*"
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        },
