{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "iiidm",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_section"
      }
    },
    "_section": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_regular_section"
        }
      ]
    },
    "_regular_section": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "present_section"
        }
      ]
    },
    "present_section": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "STRING",
            "value": "Present"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_expression"
            }
          }
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "key_value_pair"
        }
      ]
    },
    "key_value_pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_key"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "_value"
        }
      ]
    },
    "variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "_key": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable"
        }
      ]
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "number": {
      "type": "PATTERN",
      "value": "[\\d\\.]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}